<?xml version="1.0" encoding="UTF-8"?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:ProjectManagement="clr-namespace:SmartConstructionSite.ProjectManagement;assembly=SmartConstructionSite"
    xmlns:Account="clr-namespace:SmartConstructionSite.Account;assembly=SmartConstructionSite"
    x:Class="SmartConstructionSite.MainPage">

    <MasterDetailPage.Master>
        <ProjectManagement:ProjectManagementMainPage></ProjectManagement:ProjectManagementMainPage>
    </MasterDetailPage.Master>

    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <Account:UserMainPage></Account:UserMainPage>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
    
</MasterDetailPage>
